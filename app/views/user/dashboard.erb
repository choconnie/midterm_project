    <!-- Page Content -->
    <div class="container">
        <!-- Membership Feed -->
        <br>
        <div class="row">
            <div class="text-center">
                <div class="col-md-8">
					      <h2>Updates From Your Groups</h2>
					        <% if @memberships.present? %>
								<% @memberships.each do |group| %>
    								<% if group.posts.exists? %>
    								    <div class="group-message">
    									<h3><a href="/groups/<%=group.id%>/details"><%= group.group_name %></a></h3>
    									<h4><%= group.posts.last.title %></h4>
    									<p><%= group.posts.last.content %></p>
    								    </div>
    								<% end %>
							     <% end %>
							<% else %>
								<p>Please join a group if you haven't already!</p>
							<%end %>
    					</div>
                </div>
                <br><br>
            <div class="col-md-4 wb-twitter">
                <a class="twitter-timeline" 
                   width="300"
                   height="500"
                   href="https://twitter.com/ISCCanada" 
                   data-widget-id="692762832819982336">Tweets by @ISCCanada</a>
            </div>
        </div>
        <hr>
        <!-- Announcements Feed -->
        <div class="row">
            <div class="col-lg-12">
                <div class="well text-center">
					        <h3>Announcements</h3>
					        <h4><%= @announcement.title%></h4>
					        <p><%= @announcement.content%></p>
					        <p><%= @announcement.created_at %>
					      </div>
            </div>
        </div>
    <% if @events %>
    <div class="row">
    <% @events.each do |event| %>
        <div class="col-md-4">
            <p><a href="/event/<%=event.id%>/details"><%=event.title%></a></p>
            <p><%= event.details%></p>
            <p><span class="badge"><%= event.event_date%></span></p>
        </div>

    <% end %>
    </div>
    <% end %>
</div>

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

</body>




