<div class="col-md-8 col-md-offset-2 event_listing">
<h1><%= @event.title %></h1>
<h2><%= @event.event_date %></h2>
<h2><%= @event.location %></h2>
<iframe
  width="600"
  height="350"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBKhhp29pqNJNxpYTYTJL-XORw9qlxQwO8
    &q=<%=@event.location%>" allowfullscreen>
</iframe>
<h3><%= @event.url%></h3>
<p><%= @event.details %></p>
<p><%= @event.created_at %></p>
</div>