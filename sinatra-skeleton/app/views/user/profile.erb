<div class="container" style="padding-top: 60px;">
  <h1 class="page-header">Profile Information #Can add name here of whoever is login???</h1>
  <div class="row">
    <!-- left column -->
    <div class="col-md-4 col-sm-6 col-xs-12">
    <form method="post" action="/profile">
      <div class="text-center">
        <img src="http://lorempixel.com/200/200/people/4/" class="avatar img-circle img-thumbnail" alt="avatar">
        <h6>Upload a different photo...</h6>
        <input type="file" class="text-center center-block well well-sm">
      </div>
    </div>

    <div class="col-md-8 col-sm-6 col-xs-12 personal-info">
      <br><br>
      <div class="input-group add-on">
        <input type="text" class="form-control" placeholder="username" name="username">
        <div class="input-group-btn">
          <a href="/profile"><button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-edit"></i></button></a>
        </div>
      </div>
      <br>
      <div class="input-group add-on">
        <input type="text" class="form-control" placeholder="password" name="password">
        <div class="input-group-btn">
          <a href="/profile"><button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-edit"></i></button></a>
        </div>
      </div>
      <br>
      <div class="form-group">
        <label class="col-md-0 control-label"></label>
        <div class="col-md-8">
          <a href="/user/dashboard"><input class="btn btn-primary" value="Save Changes" type="button"></a>
          <span></span>
          <a href="/user/dashboard"><input class="btn btn-default" value="Cancel" type="button"></a>
        </div>
      </div>
      </form>
    </div>
  </div>
</div>




